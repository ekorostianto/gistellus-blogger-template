              <span class='uk-search-icon-flip' uk-search-icon=''/>
              <input class='uk-search-input' id='q' name='q' onblur='if(this.value==&apos;&apos;)this.value=this.defaultValue;' onfocus='if(this.value==this.defaultValue)this.value=&apos;&apos;' type='search' value='Ketik apa yang kamu cari disini...'/>
